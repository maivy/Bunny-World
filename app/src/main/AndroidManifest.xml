<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.stanford.cs108.bunnyworld">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EditOptions" />
        <activity android:name=".NewShape" />
        <activity android:name=".NewPage" />
        <activity android:name=".NewGame" />
        <activity android:name=".ViewAllShapes" />
        <activity android:name=".EditShape" />
        <activity android:name=".EditShapeOptions" />
        <activity android:name=".EditScript" />
        <activity android:name=".PlayGame"></activity>
    </application>

</manifest>